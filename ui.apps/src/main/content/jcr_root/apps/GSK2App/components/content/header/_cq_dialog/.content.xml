<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/tabs"
                    type="nav"/>
                <items jcr:primaryType="nt:unstructured">
                    <backround
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Background"
                        sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                            margin="{Boolean}false"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <bgcolor
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="bgcolor"
                                        sling:resourceType="/apps/GSKWEB62App/components/content/custom-colorpicker"
                                        class="fontcolortypeone-gsk"
                                        fieldLabel="Background Color"
                                        name="./headerBgColor"/>
                                    <fontcolor
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="fontcolor"
                                        sling:resourceType="/apps/GSKWEB62App/components/content/custom-colorpicker"
                                        fieldLabel="Font Color"
                                        name="./fontcolor"/>
                                    <image
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="image"
                                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                        fieldLabel="Image"
                                        name="./image"/>
                                    <fontsize
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="fontsize"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldLabel="Font Size"
                                        name="./fontsize"/>
                                </items>
                            </column>
                        </items>
                    </backround>
                    <text
                        jcr:primaryType="nt:unstructured"
                        jcr:title="text"
                        sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <text1
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="text1"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldLabel="Search Text"
                                        name="./text1"/>
                                    <text2
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="text2"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldLabel="Contact Text"
                                        name="./text2"/>
                                    <text3
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="text3"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldLabel="Global Text"
                                        name="./text3"/>
                                    <text4
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="text4"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldLabel="Sign In Text"
                                        name="./text4"/>
                                    <text5
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="text5"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldLabel="Nav Text 1"
                                        name="./text5"/>
                                    <text6
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="text6"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldLabel="Nav Text 2"
                                        name="./text6"/>
                                    <text7
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="text7"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldLabel="Nav Text 3"
                                        name="./text7"/>
                                </items>
                            </column>
                        </items>
                    </text>
                    <image
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Images&#xa;"
                        sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <image1
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="image1"
                                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                        fieldLabel="Search Image"
                                        name="./image1"/>
                                    <image2
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="image2"
                                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                        fieldLabel="Call Image"
                                        name="./image2"/>
                                    <image3
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="image3"
                                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                        fieldLabel="Flag Image"
                                        name="./image3"/>
                                    <image4
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="image4"
                                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                        fieldLabel="Contact Image"
                                        name="./image4"/>
                                    <image5
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="image5"
                                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                        fieldLabel="Flonase Image"
                                        name="./image5"/>
                                    <image6
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="image6"
                                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                        fieldLabel="Gsk Logo Image"
                                        name="./image6"/>
                                    <image7
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="image7"
                                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                        fieldLabel="Search"
                                        name="./image7"/>
                                </items>
                            </column>
                        </items>
                    </image>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
